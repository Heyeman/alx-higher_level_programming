#!/usr/bin/python3
def new_in_list(my_list, idx, element):
    copy = []
    for i in range(len(my_list)):
        copy.append(my_list[i])
    if idx < 0:
        return copy
    elif idx >= len(my_list):
        return copy
    else:
        copy[idx] = element
        return copy
my_list = [1, 2, 3, 4, 5]
idx = 3
new_element = 9
new_list = new_in_list(my_list, idx, new_element)

print(new_list)
print(my_list)
